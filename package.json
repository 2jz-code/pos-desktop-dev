{
  "name": "ajeen-fresh",
  "private": true,
  "workspaces": [
    "packages/*",
    "electron-app",
    "admin-site"
  ],
  "scripts": {
    "dev:electron": "cd electron-app && npm run dev",
    "dev:admin": "cd admin-site && npm run dev",
    "build:electron": "cd electron-app && npm run build",
    "build:admin": "cd admin-site && npm run build",
    "install:all": "npm install && npm run install:electron && npm run install:admin",
    "install:electron": "cd electron-app && npm install",
    "install:admin": "cd admin-site && npm install"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}